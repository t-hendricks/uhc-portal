  "scripts": {
    "start": "concurrently --kill-others --prefix=name --names=SERVER,MOCK --prefix-colors=cyan,reset --passthrough-arguments 'HOT=true yarn fec:dev-proxy --clouddotEnv prod {@}' 'mockdata/mockserver.py' --",
    "build": "fec build",
    "build:prod": "MODE=prod yarn build",
    "build:prod:monitored": "run/build-prod-monitored.sh",
    "fec:static": "fec static",
    "fec:dev-server": "fec dev",
    "fec:dev-proxy": "fec dev-proxy",
    "verify": "concurrently --group --names=BUILD,LINT,TEST 'yarn build:prod' 'NODE_OPTIONS=--max-old-space-size=4096 yarn lint-prettier' 'yarn test'",
    "insights-proxy": "make insights-proxy-check && run/insights-proxy.sh",
    "stop-insights-proxy": "run/podman-or-docker.sh kill insightsproxy; run/podman-or-docker.sh rm --force insightsproxy; true",
    "lint": "eslint src 'run/*js' --ext mjs,js,jsx,ts,tsx",
    "lint-prettier": "yarn lint && yarn prettier",
    "lint-staged": "lint-staged",
    "prettier": "prettier --check '{src,cypress,scripts}/**/*.{js,ts,mjs,jsx,tsx}' 'run/*js'",
    "prettier:fix": "prettier --write '{src,cypress,scripts}/**/*.{js,ts,mjs,jsx,tsx}' 'run/*js'",
    "test": "TZ=America/New_York LC_ALL=en_US.utf8 node --no-compilation-cache --expose-gc ./node_modules/.bin/jest --logHeapUsage --coverage ",
    "test-no-coverage": "yarn test --coverage=false",
    "test-changes": "yarn test --changedSince=main",
    "test-changes-with-enforcement": "yarn test-changes --coverageThreshold='{\"global\":{\"statements\":\"80\"}}'",
    "test-local": "yarn test --maxWorkers=50%",
    "test:debug": "node --nolazy --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --colors --verbose",
    "e2e-test": "rm -rf ./coverage/* && yarn cypress-headless --env grepTags=ci",
    "stop": "yarn stop-insights-proxy; run/podman-or-docker.sh rm --force browser; pkill --full 'concurrently.*SERVER(prevent self match){0},MOCK'; pkill --full 'concurrently.*BROWSER(prevent self match){0},TEST'; true",
    "prepare": "husky",
    "cypress-ui": "cypress open",
    "cypress-headless": "cypress run --browser chrome",
    "playwright-headless": "playwright test",
    "playwright-ui": "playwright test --ui",
    "playwright-headed": "playwright test --headed",
    "playwright-debug": "playwright test --debug",
    "playwright-report": "playwright show-report",
    "playwright-e2e-ci": "playwright test --grep @ci",
    "code-coverage": "nyc report --reporter=text-summary",
    "analyze": "rm -rf dist && env BUNDLE_ANALYZER=true yarn build:prod",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject ./dist/",
    "sentry:release": "sentry-cli --url https://glitchtip.devshift.net/ releases --org ocm --auth-token $GLITCHTIP_TOKEN --project=$SENTRY_PROJECT files $SENTRY_VERSION upload-sourcemaps dist/ --url-prefix '/apps'",
    "sentry:sourcemaps-release": "yarn sentry:sourcemaps && yarn sentry:release",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "postinstall": "ts-patch install",
    "find-circular-dependencies": "madge --circular --extensions ts,js ./src "
  },
  "insights": {
    "appname": "openshift"
  },
  "nyc": {
    "include": [
      "src/**/*"
    ]
  },
  "engines": {
    "node": ">= 18.12.0",
    "yarn": ">= 1.22.19"
